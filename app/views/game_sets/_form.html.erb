<%= form_for(@game_set) do |f| %>
  <% if @game_set.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@game_set.errors.count, "error") %> prohibited this game_set from being saved:</h2>

      <ul>
      <% @game_set.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    Date<br>
    <%= f.text_field :date, :id=>"datepicker" %>
  </div>
  <div class="field">
    Game 1<br>
    <%= f.number_field :games, :id=>"g1", multiple: true %>
  </div>
  <div class="field">
    Game 2<br>
    <%= f.number_field :games, :id=>"g2", multiple: true %>
  </div>
  <div class="field">
    Game 3<br>
    <%= f.number_field :games, :id=>"g3", multiple: true %>
  </div>
  
  <div class="field">
  	Select Bowler<br>
  <%= f.collection_select :person_id, @people, :id, :full_name, include_blank: false %>
  </div>
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
  <style type="text/css">
.ui-datepicker {
    background: #333;
    border: 1px solid #555;
    color: #EEE;
}
</style>
  <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
  <script>
  $( function() {
    $( "#datepicker" ).datepicker({dateFormat: "yy-mm-dd"});
  } );
  </script>
