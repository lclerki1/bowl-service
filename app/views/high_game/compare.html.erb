<div>
<h1>Compare High Game</h1>

<table border='1'>
	<tr>
		<th>Bowler ID#</th>
		<th>Bowler Name</th>
		<th>Game</th>
	</tr>
	
	<% @sorted.each do |person| %>
    <tr class="<%= cycle('even', 'odd') %>">
      <td><%= person["id"] %></td>
      <td><%= person["first_name"] %> <%= person["last_name"] %></td>
      <td><%= person["game"] %></td>
	</tr>
	<% end %>
</table>

</div>
