
	<p id="notice"><%= notice %></p>
<h1>List of Users</h1>

<table border="1">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Gender</th>
      
    </tr>
  </thead>

  <tbody>
    <% @people.each do |person| %>
      <tr>
        <td><%= person.first_name %></td>
        <td><%= person.last_name %></td>
        <td><%= person.gender %></td>
        <td><%= link_to 'Edit User', edit_person_path(person) %></td>
        <td><%= link_to 'User Stats', person %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bowler', new_person_path %>


